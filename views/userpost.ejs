<% include ./partials/header.ejs %>
<div class="w-full min-h-screen bg-zinc-800 px-10 py-5">
    <% user.post.forEach(function(po){ %>
    <div class="cards flex flex-wrap gap-10 ">
        <div class="card w-32 text-white bg-zinc-700 rounded-lg overflow-hidden">
            <div class="w-full h-40 bg-sky-200
             rounded-lg overflow-hidden object-cover">
            <img class="w-full h-full object-cover" src="/images/uploads/<%= po.postImage %>" alt="">
            
            </div>
            <!-- <h3 class="text-md"><%= po.name%></h3> -->
            <h3 class="text-lg mt-3"><%= po.title %></h3>
            <h5 class="text-sm"><%= po.description %></h5>
        </div>
        <% }) %>
    </div>
</div>
<% include ./partials/footer.ejs %>